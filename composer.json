{
    "name": "loyaltycorp/easy-pipeline",
    "description": "Provides an easy and powerful way to implement pipelines for anything",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "laravel/lumen-framework": "^5.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "LoyaltyCorp\\EasyPipeline\\": "src",
            "StepTheFkUp\\EasyPipeline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoyaltyCorp\\EasyPipeline\\Tests\\": "tests",
            "StepTheFkUp\\EasyPipeline\\Tests\\": "tests"
        }
    },
    "suggest": {
        "illuminate/pipeline": "To use the Illuminate Pipeline implementation"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
